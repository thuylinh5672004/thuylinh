@model IEnumerable<Lesson11Dbs.Models.Tacgia>

@{
    ViewBag.Title = "DtlIndex";
    Layout = "~/Views/Shared/Dtl_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "DtlCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DtlMaTG)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DtlTenTG)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DtlMaTG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DtlTenTG)
        </td>
        <td>
            @Html.ActionLink("DtlEdit", "DtlEdit", new { id = item.DtlMaTG.Trim() }) |
            @Html.ActionLink("DtlDetails", "DtlDetails", new { id = item.DtlMaTG.Trim() }) |
            @Html.ActionLink("DtlDelete", "DtlDelete", new { id = item.DtlMaTG.Trim() })
        </td>
    </tr>
}

</table>
